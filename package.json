{
  "name": "@sierralabs/nest-identity",
  "version": "0.4.1",
  "description": "Standard user management, roles, authentication, and ACL handling.",
  "author": "Sierra Labs",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:mocks": "TZ=UTC jest --config ./test/mocks/jest-mocks.json",
    "webpack": "webpack --config webpack.config.js",
    "predb": "docker-compose down -v --rmi local",
    "db": "docker-compose up --build -d",
    "mocks": "NODE_ENV=development npm run test:mocks"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/typeorm": "^7.1.5",
    "@sierralabs/nest-utils": "^0.2.0",
    "@types/convict": "^4.2.0",
    "aws-sdk": "^2.396.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.8.5",
    "convict": "^4.4.0",
    "fastify-formbody": "^2.0.2",
    "google-auth-library": "^3.1.0",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.6.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.16",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.118",
    "@types/node": "^14.14.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.6",
    "jest": "^26.6.3",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2",
    "supertest": "^3.3.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.5.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.7.0",
    "tslint": "5.3.2",
    "webpack": "^4.26.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  }
}
